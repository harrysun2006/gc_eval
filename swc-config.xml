<?xml version="1.0"?>
<flex-config>
	<compiler>
		<accessible>false</accessible>
		<allow-source-path-overlap>true</allow-source-path-overlap>
		<external-library-path>
		   <path-element>${flexlib}\libs\player\10\playerglobal.swc</path-element>
		   <path-element>${flexlib}\libs\</path-element>
		</external-library-path>
		<!--locale>
		   <locale-element>en_US</locale-element>
		</locale-->
		<source-path>
		   <path-element>src</path-element>
		</source-path>
		<!--source-path>
		   <path-element>locale/{locale}</path-element>
		</source-path-->
    <!--namespaces>
			<namespace>
				<uri>http://www.adobe.com/2006/mxml</uri>
				<manifest>manifest.xml</manifest>
			</namespace>
    </namespaces-->
		<strict>true</strict>
		<as3>true</as3>
		<es>false</es>
		<optimize>true</optimize>
		<verbose-stacktraces>false</verbose-stacktraces>
	</compiler>
	<output>eval-1.0.0.swc</output>
	<include-classes>
		<class>bee.eval.ast.AddExpression</class>
		<class>bee.eval.ast.CallExpression</class>
		<class>bee.eval.ast.DivExpression</class>
		<class>bee.eval.ast.IdentExpression</class>
		<class>bee.eval.ast.IExpression</class>
		<class>bee.eval.ast.MulExpression</class>
		<class>bee.eval.ast.NumberExpression</class>
		<class>bee.eval.ast.SubExpression</class>
		<class>bee.eval.ast.UnaryMinusExpression</class>
		<class>bee.eval.ast.UnaryPlusExpression</class>
		<class>bee.eval.Compiler</class>
		<class>bee.eval.Ident</class>
		<class>bee.eval.IdentType</class>
		<class>bee.eval.ParseError</class>
		<class>bee.eval.Parser</class>
		<class>bee.eval.Scanner</class>
		<class>bee.eval.SymbolTable</class>
		<class>bee.eval.Token</class>
		<class>bee.eval.TokenType</class>
		<class>bee.eval.Util</class>
	</include-classes>
